      <parameter parameterId="39d87901-14d4-49f6-90ec-4e992c07cbd6" amount="0.7" variableName="fraction_el" mathematicalRelation="1- fraction_diesel" unitId="577e242a-461f-44a7-922c-d8e1c3d2bf45">
