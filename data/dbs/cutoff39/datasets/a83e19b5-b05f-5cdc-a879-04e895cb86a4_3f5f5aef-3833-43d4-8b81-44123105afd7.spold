      <parameter parameterId="3a09a4e4-5dc4-40dd-a83e-a3b7e5ea7ada" amount="0.688524590163934" variableName="fraction_sprinkler_irrigation" unitId="577e242a-461f-44a7-922c-d8e1c3d2bf45">
