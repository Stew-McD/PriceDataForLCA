      <elementaryExchange id="82bbc0df-9271-5abb-92fe-6e639914aff4" unitId="487df68b-4994-4027-8fdc-a4dc298257b7" casNumber="007446-09-5" amount="0.0036" isCalculatedAmount="false" elementaryExchangeId="fd7aa71c-508c-480d-81a6-8052aad92646">
