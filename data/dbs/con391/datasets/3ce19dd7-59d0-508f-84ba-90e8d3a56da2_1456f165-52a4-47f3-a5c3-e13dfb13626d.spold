          <lognormal meanValue="0.0004" mu="-7.82" variance="0.12" varianceWithPedigreeUncertainty="0.16"/>
          <pedigreeMatrix reliability="1" completeness="1" temporalCorrelation="5" geographicalCorrelation="1" furtherTechnologyCorrelation="1"/>
        </uncertainty>
