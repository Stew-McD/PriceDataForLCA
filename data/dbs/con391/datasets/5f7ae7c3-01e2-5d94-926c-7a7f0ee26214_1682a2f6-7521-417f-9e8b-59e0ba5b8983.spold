      <activity specialActivityType="0" id="5f7ae7c3-01e2-5d94-926c-7a7f0ee26214" activityNameId="84771a64-b258-4ac8-bcf6-af1293247b06" parentActivityId="5de444ca-6541-4ddc-bc91-83d2ea1f5099" inheritanceDepth="-1" type="1" energyValues="0">
        <activityName xml:lang="en">chloromethyl methyl ether production</activityName>
        <synonym xml:lang="en">chloromethoxymethane</synonym>
        <synonym xml:lang="en">Chlorodimethyl ether</synonym>
        <synonym xml:lang="en">CMME</synonym>
        <synonym xml:lang="en">Monochlorodimethyl ether</synonym>
        <synonym xml:lang="en">Dimethylchloroether</synonym>
        <synonym xml:lang="en">Chlordimethylether</synonym>
