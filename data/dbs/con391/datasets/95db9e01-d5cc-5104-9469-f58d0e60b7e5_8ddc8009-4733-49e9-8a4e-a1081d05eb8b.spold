          <lognormal meanValue="0.010371102" mu="-3.318" variance="0.0006" varianceWithPedigreeUncertainty="0.0031999999999999997"/>
          <pedigreeMatrix reliability="2" completeness="1" temporalCorrelation="3" geographicalCorrelation="1" furtherTechnologyCorrelation="1"/>
        </uncertainty>
