    <activityDescription>
      <activity specialActivityType="0" id="07d4561e-ffe1-5167-9e18-d026809952c6" activityNameId="32ccb911-2e9e-43a5-a97a-02f1052ba5c1" parentActivityId="c3680c7f-b90d-5718-8f86-dd365c7c180d" inheritanceDepth="-1" type="1">
        <activityName xml:lang="en">silicon carbide production</activityName>
        <synonym xml:lang="en">carborundum</synonym>
        <synonym xml:lang="en">Carborundum</synonym>
        <synonym xml:lang="en">silicon monocarbide</synonym>
        <synonym xml:lang="en">carbolon</synonym>
        <synonym xml:lang="en">SILICON CARBIDE</synonym>
        <synonym xml:lang="en">409-21-2</synonym>
