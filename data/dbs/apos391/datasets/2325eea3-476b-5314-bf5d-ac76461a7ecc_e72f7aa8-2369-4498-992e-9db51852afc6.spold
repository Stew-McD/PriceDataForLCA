      <parameter parameterId="8740e972-cdd4-419c-9583-e234e35806d6" amount="0.000805714285714289" variableName="K2O_available" mathematicalRelation="sludge_K_content*sludge_Dry_mass*EC_K/ELF_K*K_to_K2O" unitId="487df68b-4994-4027-8fdc-a4dc298257b7">
